//Conditional statements
var age = prompt("Please enter your age before entering");
//if you are younger than 18
if (age < 18) {
console.log("Sorry you are not old enought to enter the venue");
}
//if you are between 18-21
else if (age < 21) {
console.log("You can enter, but you cannot drink");
}
//if you are over 21 
else {
console.log("You can enter and have a few!!! CONGRATS");
}
